{% include 'in_env' %}
{% include 'out_env' %}
in:
  type: mysql
  host: {{ host }}
  database: {{ database }}
  user: {{ user }}
  password: {{ password }}
  query: select * from departments;
out:
  type: bigquery
  project: {{ project }}
  location: {{ location }}
  dataset: lake
  table: departments
  mode: append
  auto_create_table: true

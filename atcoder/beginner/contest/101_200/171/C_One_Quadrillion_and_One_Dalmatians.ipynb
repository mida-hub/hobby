   "source": [
    "def num2alpha(num):\n",
    "    if num<=26:\n",
    "        return chr(64+num)\n",
    "    elif num%26==0:\n",
    "        return num2alpha(num//26-1)+chr(90)\n",
    "    else:\n",
    "        return num2alpha(num//26)+chr(64+num%26)"
   ]
  },

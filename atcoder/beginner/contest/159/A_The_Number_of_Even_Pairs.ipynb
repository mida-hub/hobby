   "source": [
    "n, m = map(int, input().split())"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 25,
   "metadata": {},
   "outputs": [],
   "source": [
    "N = [2 for x in range(n)]\n",
    "M = [1 for x in range(m)]"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 26,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "3\n"
     ]
    }
   ],
   "source": [
    "count = []\n",
    "for v in itertools.combinations(N + M, 2):\n",
    "    count.append(sum(v) % 2 == 0)\n",
    "print(sum(count))"
   ]
  }
